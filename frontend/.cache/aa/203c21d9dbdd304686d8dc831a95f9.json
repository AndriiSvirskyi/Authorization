credentials, callback) => async dispatch => {\r\n  \r\n  try {\r\n    const response = await axios.post(`${url}/signin`, credentials);\r\n    const { token } = response.data;\r\n\r\n    dispatch({\r\n      type: AUTH,\r\n      token\r\n    });\r\n    localStorage.setItem('token', token);\r\n    callback()\r\n  } catch (err) {\r\n    dispatch({\r\n      type: ERROR,\r\n      message: 'You entered incorrect data'\r\n    });\r\n  }\r\n};\r\n\r\nexport const signout = () => {\r\n  localStorage.removeItem('token');\r\n  document.location = \"http://localhost:1234\"\r\n  return {\r\n    type: AUTH,\r\n    token: ''\r\n  };\r\n};\r\n"},"lineCount":null}},"hash":"93ad36f51471a3606907cb258e6afee5","cacheData":{"env":{}}}