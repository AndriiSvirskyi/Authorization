k } from 'react-router-dom';\r\n\r\n\r\nclass Signin extends PureComponent {\r\n    onSubmit = credentials => {\r\n        this.props.signin(credentials, () => {\r\n            document.location = \"http://localhost:1234/feature\";\r\n        });\r\n    }\r\n    render() {\r\n        \r\n        return ( \r\n        <Form\r\n            onSubmit={this.onSubmit}\r\n            render={({ handleSubmit }) => (\r\n                <div className=\"form-wrap\">\r\n                    <form className=\"form\" onSubmit={handleSubmit}>\r\n\r\n                    <h1>Authorization</h1>\r\n                    <h4 style={{color: \"red\"}}>{this.props.message}</h4>\r\n                        <div>\r\n                            <label>\r\n                                <div className=\"email\">Email</div>\r\n                                <Field name=\"email\" type=\"text\" component=\"input\" autoComplete=\"off\" />\r\n                            </label>\r\n                        </div>\r\n                        <div>\r\n                            <label>\r\n                                <div className=\"password\">Password</div>\r\n                                <Field name=\"password\" type=\"password\" component=\"input\" autoComplete=\"off\"/>\r\n                            </label>\r\n                        </div>\r\n                        <div className=\"link-block\">\r\n                            <button type=\"submit\" className=\"button\">Sign In</button>\r\n                            <div className=\"button\"><Link to=\"/signup\">Sign Up</Link></div>\r\n                        </div>\r\n                    </form>\r\n                </div>\r\n            )}\r\n        />\r\n        );\r\n    }\r\n}\r\n\r\nconst mapStateToProps = state => ({\r\n    message: state.auth.message,\r\n    token: state.auth.token\r\n});\r\n\r\nexport default connect(mapStateToProps, authActions)(Signin);\r\n"},"lineCount":null}},"hash":"bf907c3c72076a668ff24fe203104a87","cacheData":{"env":{}}}